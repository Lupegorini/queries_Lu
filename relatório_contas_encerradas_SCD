---- Relatório de contas encerradas SCD ----

SELECT distinct
m.MERCHANT_CODE,
la.STATE AS BKO_STATE,
la.KYC_STATUS,
bko.CLOSING_REASON,
bko.SUSPENSION_REASON,
bko.UPDATED_AT,
mas.STATE AS SOAP_STATE,
mas.SUBSTATE AS SOAP_SUBSTATE,
bko.COUNTRY/*,
su.EMAIL,
kl.ACTION_TIME::date AS action_date,
kas.NAME AS ACTION,
kl.COMMENT*/
FROM MART_MERCHANT_BANK.V_M_DIM_MERCHANTS_LATAM la 
LEFT JOIN MART_MERCHANT_BANK.A_DIM_ACCOUNTS_ACCOUNTS_LATAM bko on la.MERCHANT_CODE = bko.SUMUP_MERCHANT_CODE 
LEFT JOIN src_payment.merchants m ON la.MERCHANT_CODE = m.MERCHANT_CODE 
LEFT JOIN src_payment.merchant_account_statuses mas ON mas.merchant_id = m.id
LEFT JOIN src_payment.merchant_account_status_details msd ON msd.merchant_account_status_id = mas.id
LEFT JOIN src_payment.kyc_logs kl ON kl.merchant_id = m.id
LEFT JOIN src_payment.kyc_actions ka ON ka.id = kl.kyc_action_id
LEFT JOIN src_payment.kyc_action_statuses kas ON kas.id = kl.kyc_action_status_id
LEFT JOIN src_payment.superusers su ON su.id = kl.operator_id
left join ANALYST_OPERATIONS.OPS_SCHEDULE sch on sch.agent = su.email and sch.date = kl.action_time::date
left join analyst_risk_and_fraud.teams_2 t2 on t2.email = su.email
WHERE 1=1
AND la.STATE IN ('closed','suspended')
AND bko.UPDATED_AT BETWEEN '2024-01-01' AND '2024-06-30'
AND bko.COUNTRY IN ('BRA')
--AND kas.id in ('35','36','38','44')
QUALIFY ROW_NUMBER() OVER (PARTITION BY m.MERCHANT_CODE ORDER BY bko.UPDATED_AT desc) = 1





----------- ACTIONS KYC ----------- 
-- CLOSED: 35
-- ACTIVE: 43
-- BLOCKED: 38
-- SUSPENDED: 36
-- COMMENT: 44
-- OPS_REVIEW: 45
-- APPROVED: 27
-- EXTERNAL_ID_CHECK_PASS: 55
-- EXTERNAL_ID_CHECK_FAIL: 56


--- comentário de encerramento --- 
SELECT distinct
m.MERCHANT_CODE,
mas.SUBSTATE AS SOAP_SUBSTATE,
bko.COUNTRY,
su.EMAIL,
kl.ACTION_TIME::date AS action_date,
kas.NAME AS ACTION,
kl.COMMENT
FROM MART_MERCHANT_BANK.V_M_DIM_MERCHANTS_LATAM la 
LEFT JOIN MART_MERCHANT_BANK.A_DIM_ACCOUNTS_ACCOUNTS_LATAM bko on la.MERCHANT_CODE = bko.SUMUP_MERCHANT_CODE 
LEFT JOIN src_payment.merchants m ON la.MERCHANT_CODE = m.MERCHANT_CODE 
LEFT JOIN src_payment.merchant_account_statuses mas ON mas.merchant_id = m.id
LEFT JOIN src_payment.merchant_account_status_details msd ON msd.merchant_account_status_id = mas.id
LEFT JOIN src_payment.kyc_logs kl ON kl.merchant_id = m.id
LEFT JOIN src_payment.kyc_actions ka ON ka.id = kl.kyc_action_id
LEFT JOIN src_payment.kyc_action_statuses kas ON kas.id = kl.kyc_action_status_id
LEFT JOIN src_payment.superusers su ON su.id = kl.operator_id
left join ANALYST_OPERATIONS.OPS_SCHEDULE sch on sch.agent = su.email and sch.date = kl.action_time::date
left join analyst_risk_and_fraud.teams_2 t2 on t2.email = su.email
WHERE 1=1
--AND kl.ACTION_TIME BETWEEN '2024-01-01' AND '2024-06-30'
AND kas.id in ('38', '35')
--AND su.email NOT IN ('null')
AND m.MERCHANT_CODE IN (xxx)
QUALIFY ROW_NUMBER() OVER (PARTITION BY kl.MERCHANT_ID ORDER BY kl.action_time desc) = 1


----- Comment em Contas BKO CLOSED + SOAP Active -----
SELECT distinct
m.MERCHANT_CODE,
mas.SUBSTATE AS SOAP_SUBSTATE,
bko.COUNTRY,
su.EMAIL,
kl.ACTION_TIME::date AS action_date,
kas.NAME AS ACTION,
kl.COMMENT
FROM MART_MERCHANT_BANK.V_M_DIM_MERCHANTS_LATAM la 
LEFT JOIN MART_MERCHANT_BANK.A_DIM_ACCOUNTS_ACCOUNTS_LATAM bko on la.MERCHANT_CODE = bko.SUMUP_MERCHANT_CODE 
LEFT JOIN src_payment.merchants m ON la.MERCHANT_CODE = m.MERCHANT_CODE 
LEFT JOIN src_payment.merchant_account_statuses mas ON mas.merchant_id = m.id
LEFT JOIN src_payment.merchant_account_status_details msd ON msd.merchant_account_status_id = mas.id
LEFT JOIN src_payment.kyc_logs kl ON kl.merchant_id = m.id
LEFT JOIN src_payment.kyc_actions ka ON ka.id = kl.kyc_action_id
LEFT JOIN src_payment.kyc_action_statuses kas ON kas.id = kl.kyc_action_status_id
LEFT JOIN src_payment.superusers su ON su.id = kl.operator_id
left join ANALYST_OPERATIONS.OPS_SCHEDULE sch on sch.agent = su.email and sch.date = kl.action_time::date
left join analyst_risk_and_fraud.teams_2 t2 on t2.email = su.email
WHERE 1=1
AND bko.COUNTRY IN ('BRA')
AND kas.id in ('44')
AND su.email NOT IN ('null')
AND kl.COMMENT not like '%Uploaded%' 
AND kl.COMMENT NOT like '%changed%' 
AND kl.COMMENT NOT like '%Changed%' 
AND kl.COMMENT NOT like '%inserted/updated%'
AND kl.COMMENT NOT like '%!_RESET_BR_BANK_STATUS_!%' 
AND kl.COMMENT NOT like '%Triggered%' 
AND kl.COMMENT NOT like '%Re-triggered%' 
AND m.MERCHANT_CODE IN ('MKUE2HPM','MCU66TY2','MR3XE2EA','MC9SMT97','MCDHY4NS','MDHUHMLX','MDD3QG3F')
QUALIFY ROW_NUMBER() OVER (PARTITION BY kl.MERCHANT_ID ORDER BY kl.action_time desc) = 1

