----- NOTIFICAÇÕES BANDEIRAS, PUXAR MCC ------
SELECT distinct 
pv.TAX_ID AS CNPJ,
b.MERCHANT_CODE_C AS MERCHANT_CODE,
dm.MERCHANT_ID,
MCC_CODE_C AS CURRENT_MCC,
a."NAME"AS FULL_NAME,
a.NATIONAL_ID_C AS CPF
from SRC_SALESFORCE.contact a 
left join SRC_SALESFORCE."ACCOUNT" b ON b.id = a.ACCOUNT_ID  
LEFT JOIN SUMUP_DWH_PROD.OLAP.V_M_DIM_MERCHANT dm ON b.MERCHANT_CODE_C = dm.MERCHANT_CODE 
LEFT JOIN  SUMUP_DWH_PROD.OLAP.V_M_DIM_MERCHANT_PRIVATE pv ON dm.DIM_MERCHANT_ID = pv.DIM_MERCHANT_ID 
where 1=1
and pv.NATIONAL_ID IN ('001.014.535-40')
--WHERE pv.TAX_ID IN ('62.130.299/0001-56')
--where b.MERCHANT_CODE_C IN (xxx)
